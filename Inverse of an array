import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int [] A=new int[n];
        for(int i=0;i<n;i++)
        {
            A[i]=sc.nextInt();
        }
        int []B=new int[n];
        for (int i=0;i<n;i++)
        {
            B[A[i]]=i;

        }
        for(int i=0;i<n;i++)
        {
            System.out.print(B[i]+" ");
        }
}
}
